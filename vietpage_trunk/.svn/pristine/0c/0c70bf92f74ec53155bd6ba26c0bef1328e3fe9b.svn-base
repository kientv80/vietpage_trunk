<table width="100%">
	<tr>
		<td>
			<table width="100%">
				<tr height="30px">
					<td width="20px" align="center">
					<a href="/vietpage?comid=${companyInfo.id }" title="About us">
						<img src="/images/vietpage/Home.png"  class="menu_button" id="aboutBtn"  onmouseover="resize('aboutBtn')" onmouseout="reloadsize('aboutBtn')"/>
						<br>Trang Chủ
					</a>
					</td>
					<td width="20px" align="center">
					<a href="/items?comid=${companyInfo.id }" title="Menu">
