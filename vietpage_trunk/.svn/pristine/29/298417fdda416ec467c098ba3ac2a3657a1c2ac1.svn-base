<%@ taglib prefix="s" uri="/struts-tags"%>
<%@ taglib prefix="ui" uri="/custom-tags"%>
<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %>
<%@ taglib uri="http://tiles.apache.org/tags-tiles" prefix="tiles" %>
<%@ taglib prefix="s" uri="/struts-tags" %>

<div style="position: relative;width: 100%;overflow: hidden;float: left;margin-bottom: 40px;">
	<div class="info_admin">
		<a href="#" style="color: white;"  id="phone">
			<img src="/images/vietpage/Phone.png" />
			${companyInfo.phone}
		</a>
	</div>
	<div class="info_admin" >
		<a href="#" id="map_image1" style="color: white;">
			<img alt="Map of the shop" src="/images/googlemap.jpg" style="widows: 64px;height: 64px;"><s:text name="vietpage.company.location"></s:text>
		</a>
	</div>
	<div class="info_admin" >
		<a href="/edit" title="Menu" id="viewMenu" style="color: white;">
			<img src="/images/vietpage/sanpham.png" id="viewMenu" /><s:text name="vietpage.page.item"></s:text>
		</a>
	</div>
</div>

<div style="position: relative;width: 100%;overflow: hidden;float: left;margin-bottom: 40px;">
	<div class="pageAdminAction" style="background-color: #0DA645">
		<a href="#" id="add" style="color: black;">
			<s:text name="vietpage.pageadmin.additem"></s:text>
		</a>
	</div>
	<div class="pageAdminAction" style="float: right;background-color: #FC890D">
		<a href="/manageFeed?comid=${companyInfo.id }" style="color: black;">
			<s:text name="vietpage.com.feed"></s:text>
		</a>
	</div>
</div>
