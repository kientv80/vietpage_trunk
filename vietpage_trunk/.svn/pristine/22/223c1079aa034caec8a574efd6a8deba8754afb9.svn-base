		</attribute>
		
		<attribute>
			<description><![CDATA[sortableColumns]]></description>
			<name>sortableColumns</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		
		<attribute>
			<description><![CDATA[returnValueOfColumns]]></description>
			<name>returnValueOfColumns</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		
		<attribute>
			<description><![CDATA[containerId]]></description>
			<name>containerId</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		
		<attribute>
			<description><![CDATA[callBackMethod]]></description>
			<name>callBackMethod</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		
		<attribute>
			<description><![CDATA[callBackButtonId]]></description>
			<name>callBackButtonId</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		
		<attribute>
			<description><![CDATA[queryJsonDataUrl]]></description>
			<name>queryJsonDataUrl</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		
		<attribute>
			<description><![CDATA[sortByColumns]]></description>
			<name>sortByColumns</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
				<attribute>
			<description><![CDATA[tableWithCheckBox]]></description>
			<name>tableWithCheckBox</name>
