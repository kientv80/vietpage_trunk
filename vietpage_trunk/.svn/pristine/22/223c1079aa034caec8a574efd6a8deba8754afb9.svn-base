	</tag>
	<tag>
		<description><![CDATA[YUI tabview]]></description>
		<name>tabview</name>
		<tag-class>com.vnsoft.server.tags.yui.YUITabViewTag</tag-class>
		<body-content>JSP</body-content>
		<attribute>
			<description><![CDATA[displayTabs format fieldName:Label,fieldName:Label]]></description>
			<name>displayTabs</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description><![CDATA[contentProviderClass is a full path class that will build the content of tab. This class must implement TabViewContentProvider interface]]></description>
			<name>contentProviderClass</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		
		<attribute>
			<description><![CDATA[data is a map<tabname,dataobject>]]></description>
			<name>data</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<type>java.util.Map</type>
		</attribute>
		
		<attribute>
			<description><![CDATA[id]]></description>
			<name>id</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description><![CDATA[id]]></description>
			<name>customParams</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description><![CDATA[tabviewContainerCSS]]></description>
			<name>tabviewContainerCSS</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description><![CDATA[tabviewContentCSS]]></description>
			<name>tabviewContentCSS</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description><![CDATA[tabviewOnSelection]]></description>
			<name>tabviewOnSelection</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		
	</tag>
	
	<tag>
		<description><![CDATA[YUI tabview]]></description>
		<name>chart</name>
		<tag-class>com.vnsoft.server.tags.yui.ChartTag</tag-class>
		<body-content>JSP</body-content>
		<attribute>
			<description><![CDATA[title of the chart]]></description>
			<name>title</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description><![CDATA[chartType: line,stepLine,spline,area,splineArea,bar,pie,doughnut,bubble,scatter,stackedColumn,stackedColumn100,stackedArea,stackedArea100,stackedBar,stackedBar100]]></description>
			<name>chartType</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description><![CDATA[containerId: div id that contains the chart]]></description>
			<name>containerId</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description><![CDATA[chartData: list of ChartData]]></description>
			<name>chartData</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<type>java.util.List</type>
		</attribute>
		<attribute>
			<description><![CDATA[xTitle]]></description>
			<name>xTitle</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description><![CDATA[yTitle]]></description>
			<name>yTitle</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
	
<tag>
		<description><![CDATA[Simple Table]]></description>
		<name>table</name>
		<tag-class>com.vnsoft.server.tags.yui.SimpleTableTag</tag-class>
		<body-content>JSP</body-content>
		<attribute>
			<description><![CDATA[headers coln1, col2,col3]]></description>
			<name>headers</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description><![CDATA[displayHeader]]></description>
			<name>displayHeader</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		
		<attribute>
			<description><![CDATA[data List<Object>]]></description>
			<name>data</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<type>java.util.List</type>
		</attribute>
		
		<attribute>
			<description><![CDATA[contentProvider]]></description>
			<name>contentProvider</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description><![CDATA[params btnPrefix=,width=,height=,callback=,editable=,showView=]]></description>
			<name>params</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
	
	<tag>
		<description><![CDATA[Rating Start]]></description>
		<name>rating</name>
		<tag-class>com.vnsoft.server.tags.yui.RatingTag</tag-class>
		<body-content>JSP</body-content>
		<attribute>
			<description><![CDATA[rating]]></description>
			<name>rating</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
	
</taglib>
